{
  "name": "adventurous-lovers",
  "version": "0.0.1",
  "description": "Fullstack Starter Project with React & Node JS",
  "main": "server.js",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.local.js --mode development --inline --progress --port=3000",
    "start:server": "nodemon ./server.js",
    "build": "npm run build:prod",
    "build:local": "npm run clean:dist && webpack --config config/webpack.local.js --mode development",
    "build:dev": "npm run clean:dist && webpack --config config/webpack.dev.js --mode development",
    "build:prod": "npm run clean:dist && webpack --config config/webpack.prod.js --mode production",
    "clean:dist": "rimraf -- dist"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sani-banani/react-starter.git"
  },
  "author": "a.konovalov88@gmail.com",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-inlinesvg": "^0.8.1",
    "react-player": "^1.6.4",
    "react-responsive-carousel": "^3.1.39",
    "react-router-dom": "^4.2.2",
    "react-slick": "^0.23.1",
    "slick-carousel": "^1.8.1",
    "video-react": "^0.13.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.2.3",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "nodemon": "^1.12.1",
    "rimraf": "^2.6.2",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.4",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.1"
  }
}
